namespace org1.andrew.lognetwork

asset LoggingDevice identified by deviceId {
  o String deviceId
  o String log
  o String logCount
  o DateTime timeEntered
}

transaction AddNewLog {
  o String deviceId
  o String newLog
  o String logCount
}

@commit(false)
@returns(LoggingDevice[])
transaction DeviceLogHistory {
  o String deviceId
}
